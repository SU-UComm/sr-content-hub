{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "React app starter made on Squiz Boilerplate",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "webpack --config webpack/webpack.prod.js --env.minify=false",
    "build-min": "webpack --config webpack/webpack.prod.js --env.minify=true",
    "serve": "webpack-dev-server --open --config webpack/webpack.dev.js",
    "add-module": "bash scripts/add_module.sh",
    "lint": "npm-run-all lint:js lint:css",
    "lint:js": "eslint -f codeframe \"./src/**/*.js\" \"./src/**/*.jsx\"",
    "lint:css": "stylelint \"./src/**/*.css\" \"./src/**/*.scss\" \"!./src/**/*.min.css\" \"!./src/templates/**/*.scss\"",
    "format": "prettier --ignore-path .prettierignore --write \"./src/modules/**/*.+(js|jsx|json)\"",
    "cypress:install": "npm install -sD axe-core cypress@4.0.1 cypress-axe",
    "cypress:verify": "cypress verify",
    "cypress:tests": "npx cypress open",
    "cypress:ci": "cypress run --browser chrome --headless"
  },
  "moduleNameMapper": {},
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/preset-env": "^7.10.1",
    "@babel/preset-react": "^7.10.1",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "autoprefixer": "^9.7.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^5.0.5",
    "core-js": "^3.4.2",
    "css-loader": "^1.0.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-react": "^7.20.0",
    "file-loader": "^2.0.0",
    "glob": "^7.1.6",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "import-glob-loader": "^1.1.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.2.1",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.9.3",
    "stylelint-webpack-plugin": "^0.10.5",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.8.2",
    "react-svg-loader": "^3.0.3",
    "webpack-merge": "^4.2.2"
  }
}
